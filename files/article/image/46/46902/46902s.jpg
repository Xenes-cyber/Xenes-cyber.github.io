        $(document).ready(function()
        {
            function slide()
            {
                $.get(
                        "/Ajax/TwitterSquare.ashx",
                        {
                            ajaxMethod: "getLastedTwitter",
                            type: 0,
                            random: Math.random()
                        },
                        function(data)
                        {
                            if (data.result)
                            {
                                var firstJq = $("#div_tweets div.list:first");
                                if (firstJq.attr("id") != data.twitterId)
                                {
                                    $(data.twitterInfoHtml).insertBefore(firstJq);
                                    var moveJq = $("#div_tweets div.list:first");
                                    moveJq.css({ marginTop: -moveJq.outerHeight(), opacity: 0 })
                                    moveJq.animate({ marginTop: 0 }, 1500).fadeTo(500, 1);
                                    $("#div_tweets div.list:last").remove();
                                }
                            }
                        },
                        "json"
                     );
            }
            var tt = window.setInterval(slide, 5000);
            $("#div_tweets").mouseover(function()
            {
                window.clearInterval(tt);
            });
            $("#div_tweets").mouseout(function()
            {
                tt = window.setInterval(slide, 5000);
            });
        });
    </script>
